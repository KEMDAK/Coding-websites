import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.PrintWriter;

public class Division_725 {

	public static void main(String[] args) throws NumberFormatException, IOException {
		BufferedReader in = new BufferedReader(new InputStreamReader(System.in));
		PrintWriter out = new PrintWriter(System.out);
		
		int c = 0;
		
		while(true){
			int N = Integer.parseInt(in.readLine());
			
			if(N == 0)
				break;
			
			if(c != 0)
				out.println("\n");
			c++;
			
			boolean valid = false;
			
			for (int i = 0; i < 10; i++) {
				for (int j = 0; j < 10; j++) {
					if(j == i) continue;
					for (int j2 = 0; j2 < 10; j2++) {
						if(j2 == i || j2 == j) continue;
						for (int k = 0; k < 10; k++) {
							if(k == i || k == j || k == j2) continue;
							for (int k2 = 0; k2 < 10; k2++) {
								if(k2 == i || k2 == j || k2 == j2 || k2 == k) continue;
								
								int d = 0;
								d += i * Math.pow(10, 4);
								d += j * Math.pow(10, 3);
								d += j2 * Math.pow(10, 2);
								d += k * Math.pow(10, 1);
								d += k2 * Math.pow(10, 0);
								
								int u = d * N;
								
								if(isDiff(u)){
									valid = true;
									out.printf("%d / %d = %d\n", u, d, N);
								}
							}
						}
					}
				}
			}
			
			if(!valid)
				out.printf("There are no solutions for %d\n", N);
			
			
		}
		
		out.flush();
		out.close();
	}

	public static boolean isDiff(int u) {
		short mask = 0;
		
		while(u != 0){
			if((mask & (1 << (u % 10))) != 0)
				return false;
			
			mask |= (1 << (u % 10));
			u /= 10;
		}
		
		return true;
	}
}
