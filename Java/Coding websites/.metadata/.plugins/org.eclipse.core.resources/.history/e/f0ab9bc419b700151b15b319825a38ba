import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.io.PrintWriter;
import java.util.ArrayList;
import java.util.StringTokenizer;

public class StationBalance_410 {
	
	public static void main(String[] args) throws IOException {
		BufferedReader in = new BufferedReader(new InputStreamReader(System.in));
		PrintWriter out = new PrintWriter(System.out);
		
//		long curt = System.currentTimeMillis();//////////////////////////////////
//		while(System.currentTimeMillis() - curt < 3000){///////////////////////////
//			
//		}////////////////////////////
//		System.out.println("done");
		
		int set = 1;
		
		while(in.ready()){
			StringTokenizer st = new StringTokenizer(in.readLine());
			
			int c = Integer.parseInt(st.nextToken());
			int s = Integer.parseInt(st.nextToken());
			
			st = new StringTokenizer(in.readLine());
			
			ArrayList<Integer> specimen = new ArrayList<>();
			
			int sum = 0;
			for (int i = 0; i < s; i++) {
				int cur = Integer.parseInt(st.nextToken()); 
				specimen.add(cur);
				sum += cur;
			}
			
			if(set != 1)
				out.println();
			
			double avg = sum / (c * 1.0);
			
//			out.printf("Set #%d\n", set++);
//			
//			for (int i = 0; i < minCost.length; i++) {
//				out.printf(" %d:", i);
//				for (int j = 0; j < minCost[i].length; j++) {
//					if(minCost[i][j] != 0)
//						out.print(" " + minCost[i][j]);
//				}
//				out.print("\n");
//			}
//			
//			out.printf("IMBALANCE = %.5f\n", IMBALANCE(minCost));
		}
		
		out.flush();
		out.close();
	}
}
